{
  "protocolName": "mongo-db-protocol",
  "description": "A DB protocol for mongo and Meta-System",
  "entrypoint": "./dist/index.js",
  "className": "MongoDbProtocol",
  "configurationFormat": {
    "dbConnectionString": { "type": "string", "required": true },
    "databaseName": { "type": "string", "required": true }
  },
  "functionDefinitions": [
    {
      "input": {
        "idString" : { "type": "string", "required": true }
      },
      "output": {
        "objectId" : { "type": "cloudedObject", "required": false },
        "errorMessage" : { "type": "string", "required": false }
      },
      "functionName": "createObjectId",
      "description": "Creates an ObjectId from a 12 character string or 24 character hex id"
    }
  ]
}